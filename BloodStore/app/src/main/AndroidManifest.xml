<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.main.bloodstore.bloodstore">

    <uses-permission android:name="android.permission.CALL_PHONE" />

    <application
        android:name=".BloodStore"
        android:allowBackup="true"
        android:icon="@drawable/imgback"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Login">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Register" />
        <activity android:name=".Search" />
        <activity android:name=".DisplayList" />
        <activity android:name=".Edit" />
        <activity android:name=".EditDetails" />
        <activity android:name=".DisplayDetails" />
        <activity android:name=".Contact_Us" />
        <activity android:name=".Tips" />
        <activity android:name=".EditPersonalDetails" />
    </application>

</manifest>